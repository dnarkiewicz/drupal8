{
  "name": "my-subtheme",
  "version": "1.0.0",
  "dependencies": {
    "uswds": "^1.1.0"
  },
  "scripts": {
    "scss:raw": "node-sass sass/uswds.scss > assets/css/uswds.css",
    "scss:compressed": "node-sass --output-style compressed sass/uswds.scss > assets/css/uswds.min.css",
    "scss": "npm run scss:raw && npm run scss:compressed"
  }
}
